<h1 class="pt-4 pb-4"  class="pt-4 pb-4">Login</h1>

<%= bootstrap_form_tag url: "/login" do |f| %>
  
  <div class="field form-group mb-3">
    <%= f.email_field :email %>
  </div>
  <div class="field form-group mb-3">
    <%= f.password_field :password %>
  </div>

  

  <div class="actions">
    <%= submit_tag "Login", :class => "btn btn-primary" %>
  </div>

  
<% end %>
